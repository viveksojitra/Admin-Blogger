  <!-- Include TinyMCE Script -->
  <script>
    tinymce.init({
      selector: '#content',
      plugins: 'advlist autolink lists link image charmap print preview hr anchor pagebreak',
      toolbar_mode: 'floating',
      height: 300,
      menubar: false,
    });
  </script>

  <!-- Sidenav Scrollbar -->
  <script>
    var win = navigator.platform.indexOf('Win') > -1;
    if (win && document.querySelector('#sidenav-scrollbar')) {
      var options = {
        damping: '0.5'
      }
      Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
    }
  </script>

  <!-- Flash Message Toast -->
  <script>
    const flashToast = document.getElementById('flash-message');
    if (flashToast) {
      const bsToast = new bootstrap.Toast(flashToast);
      bsToast.show();

      setTimeout(function() {
        bsToast.hide();
      }, 2000);
    }
  </script>

  <!-- Github buttons -->
