    <div id="flash-message" class="flash-message alert alert-success alert-dismissible fade show position-absolute start-50 p-2 px-3 text-white mt-4" role="alert">
      <%= success %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } else if (error != '') { %>
    <div id="flash-message" class="flash-message alert alert-danger alert-dismissible fade show position-absolute w-100 start-50 p-2 px-3 text-white mt-4" role="alert">
      <%= error %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
