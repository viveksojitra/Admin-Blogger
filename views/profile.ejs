  <!-- Change Password Modal -->
  <script>
    // Get the button and container elements
    const btnCp = document.getElementById('btn-cp');
    const cpContainer = document.getElementById('cp-container');

    // Add click event listener to the button
    btnCp.addEventListener('click', function() {
      cpContainer.classList.toggle('show');
    });
  </script>

  <!-- Flash Message Toast -->
  <script>
    const flashToast = document.getElementById('flash-message');
    if (flashToast) {
      const bsToast = new bootstrap.Toast(flashToast);
      bsToast.show();

      setTimeout(function() {
        bsToast.hide();
      }, 2000);
    }
  </script>

